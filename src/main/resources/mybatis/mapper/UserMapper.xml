<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jimbolix.dao.UserMapper">
    <select id="findById" parameterType="string" resultType="com.jimbolix.entity.UserInfo">
        SELECT * from user_info t where t.id=#{id}
    </select>
</mapper>